public class ClaimCorrespondences {
    public static Correspondence__c[] create(Claim__c[] cc, Map<ID, Claim__c> oldCc) {
		Logger.debug('>> ClaimCorrespondences :: create');

        cc = [
            SELECT  ID,
            Stage__c,
            Customer_Is_Owner__c,
            Response_Customer_Opposition__c,
            Cover_Note_Scope__c,
            Cover_Note__c,
            Lawsuit_Date__c,
            Response_Legal_Opposition__c,
            Result__c,
            Opportunity__r.Sales_Type__c,
            Opportunity__r.Assignment_Type__c,
            Opportunity__r.RecordTypeId,
            Opportunity__r.Priority_Level__c,
            Opportunity__r.Insurance__r.External_Id__c,
            Opportunity__r.Insurance_Status__c,
            Opportunity__r.Has_Legal_Protection_Insurance__c,
            (SELECT ID, Type__c, Description__c FROM Correspondencies__r)
            FROM Claim__c WHERE ID IN :cc
        ];

        Correspondence__c[] crr = new Correspondence__c[]{};
        for (Claim__c c: cc) {
            if (c.Stage__c == 'Verteidigungsanzeige') { // Verteidigungsanzeige
                if (c.Result__c != 'Accepted' && c.Result__c != 'Partially won') {
                    continue;
                }
                if (c.Cover_Note_Scope__c != 'Appeal') {
                    continue;
                }
                Logger.debug('here1');
                String s = 'Deckungsanfrage Verteidigungsanzeige';
                Correspondence__c cr = Correspondences.build(c, 'Legal Protection Insurance', s, 'Draft', null);
                if (cr != null) {
                    Logger.debug('here2');
                    crr.add(cr);
                }
                if (c.Cover_Note__c != 'Issued') {
                    continue;
                }
                cr = Correspondences.build(c, 'Legal Protection Insurance', 'Verteidigungsanzeige', 'Draft', null);
                if (cr != null) {
                    crr.add(cr);
                }
                // https://salesfive.atlassian.net/jira/software/projects/VH24/boards/25?selectedIssue=VH24-60
                cr = Correspondences.build(c, 'Court', 'Berufungserwiderung', 'Draft', null);
                if (cr != null) {
                    crr.add(cr);
                }
                cr = Correspondences.build(c, 'Legal Protection Insurance', 'Fristverlängerung', 'Draft', null);
                if (cr != null) {
                    crr.add(cr);
                }
            }
            else if (c.stage__c == 'Customers Opposition') { // Kundenwiderspruch
                if (Opportunities.isFinanzierung(c.opportunity__r.recordTypeId)) {
                    Correspondence__c cr = Correspondences.build(c, 'Bank', 'Widerspruch durch Kunden', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                    /*
                    // commented out to fix this trellocard: https://trello.com/c/uc2Fx1IU/399-change-correspondence-logic-fz-feedback-0805
                    if (c.opportunity__r.assignment_Type__c == 'RSV') {
                        cr = Correspondences.build(c, 'Bank', 'Widerspruch durch Anwalt', 'Draft', null);
                        if (cr != null) {
                            crr.add(cr);
                        }
                    }
					*/
                } else if (Opportunities.isLebensversicherung(c.opportunity__r.recordTypeId)) {
                    if (c.opportunity__r.priority_Level__c == 'III') {
                        if (c.opportunity__r.Insurance_Status__c == 'noch aktiv' || c.opportunity__r.Insurance_Status__c == 'beitragsfrei gestellt') {
                            if (c.Opportunity__r.Has_Legal_Protection_Insurance__c == true) {
                                Correspondence__c cr = Correspondences.build(c, 'Insurance', 'Widerspruch durch Kunden', 'Draft', null);
                                if (cr != null) {
                                    crr.add(cr);
                                }
                            }
                        }
                    }
                }
            }
            else if (c.stage__c == 'Legal Opposition') { // Anwaltswiderspruch
                Correspondence__c cr;
                if (Opportunities.isFinanzierung(c.opportunity__r.recordTypeId)) {
                    if (c.opportunity__r.assignment_Type__c == 'RSV') {
                        Logger.debug('here13 :: c.result__c != oldCc.get(c.ID).result__c: ' + c.result__c + ' ' + oldCc.get(c.ID).result__c + ' Cover_Note__c: ' + c.Cover_Note__c);

                        cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Widerspruch', 'Draft', null);
                        if (cr != null) {
                            crr.add(cr);
                        }
                        // https://trello.com/c/7ZB38BNM/504-04-fz-korrespondenz-widerspruch-durch-anwalt
                        // todo: clarify if we want it to be created whenever result__c or cover_note__c change
                        if (oldCc != null && (c.result__c != oldCc.get(c.ID).result__c || c.cover_note__c != oldCc.get(c.ID).cover_note__c)) {
                        	cr = Correspondences.build(c, 'Legal Protection Insurance', 'Widerspruch durch Anwalt', 'Draft', null);
                            if (cr != null) {
                                crr.add(cr);
                            }   
                        }
                    }
                } else if (Opportunities.isLebensversicherung(c.Opportunity__r.recordTypeId)) {
                    cr = Correspondences.build(c, 'Insurance', 'Widerspruch durch Anwalt', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                    if (c.result__c == 'Declined') {
                        if (c.Opportunity__r.Has_Legal_Protection_Insurance__c == true) {
                            cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Widerspruch', 'Draft', null);
                            if (cr != null){
                                crr.add(cr);
                            }   
                        }
                        if (c.opportunity__r.priority_Level__c == 'II' || c.opportunity__r.priority_Level__c == 'II') {
                            if (c.Opportunity__r.Has_Legal_Protection_Insurance__c == false) {
                                cr = Correspondences.build(c, 'Court', 'Klageschrift', 'Draft', null);
                                if (cr != null) {
                                    crr.add(cr);
                                }
                            }
                        }
                    }
                }
            }
            else if (c.stage__c == 'Claim Letter') { // Anspruchsschreiben
                if (c.opportunity__r.assignment_Type__c == 'RSV') {
                    Correspondence__c cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Anspruchsschreiben', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                }

                Correspondence__c cr = Correspondences.build(c, 'Vehicle Brand', 'Anspruchsschreiben', 'Draft', null);
                if (cr != null) {
                    crr.add(cr);
                }
            }
            else if (c.stage__c == 'Lawsuit') { // Klage
                if (Opportunities.isFinanzierung(c.opportunity__r.recordTypeId)) {
                    if (c.lawsuit_Date__c != null) {
                        if (c.opportunity__r.assignment_Type__c == 'RSV'
                            && (c.cover_Note_Scope__c == null
                                || !c.cover_Note_Scope__c.contains('Extrajudicial'))) {
                            Correspondence__c cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Klage', 'Draft', null);
                            if (cr != null){
                                crr.add(cr);
                            }
                        }
                    }
                }
                if (Opportunities.isDieselSkandal(c.opportunity__r.recordTypeId)) {
                    Correspondence__c cr = Correspondences.build(c, 'Court', 'Klageschrift', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }

                    if (c.lawsuit_Date__c != null) {
                        /*
                        cr = Correspondences.build(c, 'Legal Protection Insurance', 'Gerichtskostenrechnung', 'Draft', null);
                        if (cr != null) {
                            crr.add(cr);
                        }
                        */

                        // Ausnahme, falls Klageschrift schon bei Umstellung versendet wurde
                        if (c.opportunity__r.assignment_Type__c == 'RSV'
                            && (c.cover_Note_Scope__c == null
                                || !c.cover_Note_Scope__c.contains('Extrajudicial'))) {
                            cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Klage', 'Draft', null);
                            if (cr != null){
                                crr.add(cr);
                            }
                        }
                    }
                    continue;
                }

                // FZ/LV
                Correspondence__c cr = Correspondences.build(c, 'Court', 'Klageschrift', 'Draft', null);
                if (cr != null) {
                    crr.add(cr);
                }

                /*
                if (c.lawsuit_Date__c != null) {
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Gerichtskostenrechnung', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                }
                */
            }
            else if (c.stage__c == 'Appeal') { // Berufung
                // DG/FZ/LV

				// Note that Correspondences.build not create duplicates.
                Correspondence__c cr = Correspondences.build(c, 'Court', 'Replik', 'Draft', null);
                if (cr != null){
                    crr.add(cr);
                }
                
                // https://salesfive.atlassian.net/browse/VH24-42
                if (c.Result__c == 'Declined' && !String.isBlank(c.Cover_Note__c)) {
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Deckungsanfrage für Berufung', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                }

                if (c.Cover_Note__c != null && c.Cover_Note__c != '' && c.Cover_Note_Scope__c == 'Appeal') {
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Berufungsbegründung', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Berufungsschrift', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                }

                /*
                // https://trello.com/c/D8uZArrU/337-change-logic-of-correspondence-creation#comment-5ce7d1d8843af543e189bcc0
                // remove conditions for berufungsbegründung und berufungsschrift
                if (c.Cover_Note__c != 'Issued') {
                    continue;
                }
                if (c.Result__c != oldCc.get(c.ID).Result__c) {
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Berufungsbegründung', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                    cr = Correspondences.build(c, 'Legal Protection Insurance', 'Berufungsschrift', 'Draft', null);
                    if (cr != null) {
                        crr.add(cr);
                    }
                }
				*/
            }
        }

        // Insert
        if (!crr.isEmpty()) {
            Logger.debug('here3: ' + crr);
            insert crr;
        }
        return crr;
    }
    
