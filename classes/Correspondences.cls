public class Correspondences {
    // TODO:
    // correspondence has different recordtypes than opportunity check:
    // SELECT ID, DeveloperName, sobjecttype FROM RecordType where sobjecttype = 'Correspondence__c'
    private static String dkRtName = 'Dieselskandal'; // 0120Y000000W0u6
    private static String fzRtName = 'Finanzierung';
    private static String lvRtName = 'Lebensversicherung'; // 0120Y000000VvEg
    public static ID dkRtID;
    public static ID fzRtID;
    public static ID lvRtID;
    static {
        RecordType[] rts = [
            SELECT ID, DeveloperName
            FROM RecordType
            WHERE (
                DeveloperName = :dkRtName
                OR DeveloperName = :fzRtName
                OR DeveloperName = :lvRtName
            )
            AND SObjectType = 'Correspondence__c'
        ];
        for (RecordType rt : rts) {
            if (rt.developerName == dkRtName) {
                dkRtID = rt.ID;
                continue;
            }
            if (rt.developerName == fzRtName) {
                fzRtID = rt.ID;
                continue;
            }
            if (rt.developerName == lvRtName) {
                lvRtID = rt.ID;
                continue;
            }
        }
    }

    public static Id dieselSkandalRecordType() {
        return dkRtID;
    }

    public static Boolean isDieselSkandal(ID corrRecordTypeID) {
        if (corrRecordTypeID == null) {
            return false;
        }
        return corrRecordTypeID == dkRtID;
    }

    public static Id finanzierungRecordType() {
        return fzRtID;
    }

    public static Boolean isFinanzierung(ID corrRecordTypeID) {
        if (corrRecordTypeID == null) {
            return false;
        }
        return corrRecordTypeID == fzRtID;
    }

    public static Id lebensversicherungRecordType() {
        return lvRtID;
    }

    public static Boolean isLebensversicherung(ID corrRecordTypeID) {
        if (corrRecordTypeID == null) {
            return false;
        }
        return corrRecordTypeID == lvRtID;
    }

    public class InvalidInputException extends Exception {}
    public static void createCorrespondence(Correspondence__c[] crr) {
        System.debug('>> createCorrespondence');
        
        List<Correspondence__c> crrToUpdate = new List<Correspondence__c>();
        for (Correspondence__c c : crr) {
            if (c.Type__c != 'Klageschrift') {
                continue;
            }
            if (c.Stage__c != 'Sent') {
                continue;
            }
            Claim__c cl = [SELECT Id, lawsuit_Date__c FROM Claim__c WHERE Id = :c.Claim__c];
            if (cl.lawsuit_Date__c == null) {
                continue;
            }
            List<Correspondence__c> gerichtCorrs = [SELECT Id, Type__c FROM Correspondence__c WHERE Type__c = 'Gerichtskostenrechnung' AND Claim__c = :cl.Id];
            if (gerichtCorrs.size() > 0) { // don't create it twice per claim
                continue;
            }
            Correspondence__c cr = Correspondences.build(cl, 'Legal Protection Insurance', 'Gerichtskostenrechnung', 'Draft', null);
            if (cr != null) {
                crrToUpdate.add(cr);
            }
        }
        if (!crrToUpdate.isEmpty()) {
            System.debug('createCorrespondence :: inserting');
            insert crrToUpdate;
        }
        
        System.debug('<< createCorrespondence');
    }

    public static Correspondence__c build(Claim__c claim,
                                          String partner,
                                          String type,
                                          String stage,
                                          String textModules) {
        Logger.debug('>> Correspondences :: build :: claim: ' + claim);

        if (claim == null) {
            throw new InvalidInputException('Claim is mandatory');
        }

        Correspondence__c cr = new Correspondence__c(
            Claim__c = claim.Id,
            Partner__c = partner,
            Type__c = type,
            Stage__c = stage,
            Text_Modules__c = textModules
        );

        // Check for duplicates
        for (Correspondence__c ecr : claim.correspondencies__r){
            if (cr.type__c == ecr.type__c && cr.description__c == ecr.description__c){
                // Found duplicate, do not build the object
                return null;
            }
        }
        
        Logger.debug('<< Correspondences :: build :: cr: ' + cr);

        return cr;
    }
    
    public static void setTerm(Correspondence__c[] crr, Map<ID, Correspondence__c> oldCrr, Boolean isUpdate) {
        for (Correspondence__c cr: crr) {
            if (cr.term__c == null || (isUpdate && cr.date__c != oldCrr.get(cr.ID).date__c)) {
                cr.term__c = cr.date__c + (Integer)cr.follow_up_Period__c;
            }
        }
    }
    
    @future (callout=true)
    public static void autoCreateDocuments(Set<Id> ids) {
        Correspondence__c[] crr = [select Id, Type__c from Correspondence__c where Id in :ids];
        for (Correspondence__c cr: crr) {
            if (cr.type__c == 'Widerspruch durch Kunden') {
                DocumentCreationAction.autoCreateDocument(cr.Id,false,'pdf',null,null);
            }
        }
    }
    
    // Legacy, todo: refactor
    public static void autoSendMails(Correspondence__c[] crr, Map<ID, Correspondence__c> oldCrr) {
        for (Correspondence__c cr : crr) {
            if ((cr.stage__c == 'Sent' || cr.stage__c == 'Received')
                && oldCrr.get(cr.ID).stage__c != 'Sent'
                && oldCrr.get(cr.ID).stage__c != 'Received'
                && cr.partner__c != 'Customer'
                && cr.inform_Customer__c == true) {
                String templateId;
                if (cr.Opportunity_Record_Type_Id__c == Opportunities.dieselSkandalRecordType()) {
                    // Diesel
                    if (cr.Opportunity_Legal_Account_Id__c == Accounts.JCLegalID) {
                        // Jäger
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.DieselJaegerLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.Legal21ID) {
                        // 21Legal
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.Diesel21LegalLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.CLLBLegalID) {
                        // Kainz
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.DieselKainzLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.LeComLegalID) {
                        // LeCom
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.DieselLeComLegalCorrespondenceCustomerInformation));
                    } else {
                        // Lachmair
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.DieselLachmairLegalCorrespondenceCustomerInformation));
                    }
                } else if (cr.Opportunity_Record_Type_Id__c == Opportunities.finanzierungRecordType()) {
                    // FZ
                    if (cr.Opportunity_Legal_Account_Id__c == Accounts.JCLegalID) {
                        // Jäger
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.FzJaegerLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.LeComLegalID) {
                        // LeCom
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.FzLeComLegalCorrespondenceCustomerInformation));
                    } else {
                        // Kainz
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.FzKainzLegalCorrespondenceCustomerInformation));
                    }
                } else {
                    // LV
                    if (cr.Opportunity_Legal_Account_Id__c == Accounts.Legal21ID) {
                        // 21Legal
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.Lv21LegalLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.CLLBLegalID) {
                        // Kainz
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.LvKainzLegalCorrespondenceCustomerInformation));
                    } else if (cr.Opportunity_Legal_Account_Id__c == Accounts.LeComLegalID) {
                        // LeCom
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.LvLeComLegalCorrespondenceCustomerInformation));
                    } else {
                        // Jäger
                        templateId = Id.valueOf(EmailTemplates.idOf(EmailTemplates.LegalCorrespondence.LvJaegerLegalCorrespondenceCustomerInformation));
                    }
                }

                EmailUtilities emailUtilities = new EmailUtilities(cr.Id, cr.Contact_Id__c, templateId, UserInfo.getUserId());
                String result = emailUtilities.send();
                System.debug('Sent automatic mail with result: ' + result);
            }
        }
    }
    
