    public string title {
        get {
            if (this.o == null) {
                return '';
            }
            
            if (Opportunities.isLebensversicherung(this.o.RecordTypeID)) {
                return 'Lebensversicherung';
            } else if (Opportunities.isDieselSkandal(this.o.RecordTypeID)) {
                return 'Dieselskandal';
            } else {
                return '';
            }
        }
        set;
    }

    public boolean getRenderLv() {
        if (this.o == null) {
            return false;
        }
        return Opportunities.isLebensversicherung(this.o.RecordTypeID);
    }

    private GoogleDriveFile uploadAttachment(string name, string contentType, blob data) {
        if (this.o == null) {
            return null;
        }
        return GoogleDriveUtils.forAccount(this.o.Account_Id__c).uploadFile(name, contentType, data);
    }

    private void UploadLv() {
        List<GoogleDriveTypeFile> googleTypeFiles = new List<GoogleDriveTypeFile>();
        if (this.authDataLv != null) {
            googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.authNameLv, this.authContentTypeLv, this.authDataLv),
                    'Vollmacht'
                )
            );
            errorNoDocuments = false;
        }
        if (this.otherData != null) {
            googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.otherName, this.otherContentType, this.otherData),
                    'Other'
                )
            );
            // errorNoDocuments = false; // is not required
        }
        if (this.note != null) {
            this.o.Bemerkung_LV__c = this.note;
            update this.o;
        }
        storeGoogleFiles(googleTypeFiles);
    }

    private void UploadNotLv() {
        List<GoogleDriveTypeFile> googleTypeFiles = new List<GoogleDriveTypeFile>();
        if (this.registrationData != null) {
            googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.registrationName, this.registrationContentType, this.registrationData),
                    'Other'
                )
            );
            errorNoDocuments = false;
        }
        if (this.invoiceData != null) {
            googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.invoiceName, this.invoiceContentType, this.invoiceData),
                    'Other'
                )
            );
            errorNoDocuments = false;
        }
        if (this.confirmationData != null) {
           googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.confirmationName, this.confirmationContentType, this.confirmationData),
                    'Other'
                )
            );
            errorNoDocuments = false;
        }
        if (this.authData != null) {
            googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.authName, this.authContentType, this.authData),
                    'Vollmacht'
                )
            );
            errorNoDocuments = false;
        }
        if (this.loanData != null) {
           googleTypeFiles.add(
                new GoogleDriveTypeFile(
                    this.uploadAttachment(this.loanName, this.loanContentType, this.loanData),
                    'Other'
                )
            );
            errorNoDocuments = false;
        }
        storeGoogleFiles(googleTypeFiles);
    }

    @testvisible
    private static string truncFilename(string file) {
        return truncFilename(file, 60);
    }

    private static string truncFilename(string file, integer truncLen) {
        integer MAX_EXTENSION_LEN = 5;
        integer flen = file.length();
        if (flen > truncLen) {
            integer dot = file.lastIndexOf('.');
            if (dot == -1) { // no extension
                return file.subString(0,truncLen) + '...';
            }
            integer diff = flen - dot;
            if (diff > MAX_EXTENSION_LEN) { // to long extension
                return file.subString(0,truncLen) + '...';
            }
            string extension = file.subString(dot - 1);
            return file.subString(0,truncLen) + '...' + extension;
        }
        return file;
    }

    private void storeGoogleFiles(List<GoogleDriveTypeFile> files) {
        List<Document__c> docs = new List<Document__c>();
        for (GoogleDriveTypeFile f : files) {
            if (f == null) {
                continue;
            }  
            string truncName = truncFilename(f.file.filename, 60);
            string truncNameLong = truncFilename(f.file.filename, 1000);
            Document__c doc = new Document__c(
                Opportunity__c = this.o.id,
                Link__c = f.file.url,
                Google_Drive_Id__c = f.file.id,
                Name = truncName,
                Long_Name__c = truncNameLong,
                Type__c = f.typ
            );
            docs.add(doc);
        }
        insert docs;
    }

    public PageReference upload() {
        errorNoDocuments = true;
        if (getRenderLv()) {
            UploadLv();
        } else {
            UploadNotLv();
        }
        if (errorNoDocuments) {
            return new PageReference('/MissingDocuments?token=' + getOppUniqueId());
        }
        return new PageReference('/DocumentsUploaded');
    }

    public PageReference init() {
        this.o = getOpportunity(); 
        if (this.o == null) {
            return new PageReference('/notfound');
        } 
        return null;
    }

    private Opportunity getOpportunity() {
        string oppId = getOppUniqueId();
        if (oppId == null) {
            System.debug('mi oppid is nukll');
            return null;
        }
        List<Opportunity> opps = [
            SELECT Id, RecordTypeID, Account_Id__c
            FROM Opportunity
            WHERE Token__c = :oppId
        ];
        if (opps.size() == 0) {
            return null;
        }
        return opps[0];
    }

    private string getOppUniqueId() {
        return ApexPages.currentPage().getParameters().get('token');
    }
    
    public class Vollmacht {
        public string name { get; set; }
        public string url { get; set; }
        public string id { get; set; }
    }
    
    public List<Vollmacht> getVollmachts() {
        
        List<Authorization__c> auths = [
            SELECT id
            FROM Authorization__c
            WHERE Opportunity__c = :this.o.Id
        ];
        
        
        List<Vollmacht> vm = new List<Vollmacht>();
        
        for(Authorization__c a : auths) {
            Vollmacht m = getVollmachtFromAuth(a.Id);
            if (m != null) {
                vm.add(m);
            }
        }
        return vm;
    }
    
    private Vollmacht getVollmachtFromAuth(string authId) {
        List<Attachment> atts = [
            SELECT Name, Id
            FROM Attachment
            WHERE ParentId = :authId
        	LIMIT 1    
        ];
        if (atts.size() == 0) {
            return null;
        }
        Attachment a = atts[0];
        
        Vollmacht m = new Vollmacht();
        m.name = a.Name;
        m.url = '/servlet/servlet.FileDownload?file=' + a.Id;
        m.id = a.Id;
        
        return m;
    }
    
