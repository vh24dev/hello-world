// Cancellation__c "Stornierung versenden" Button
public class SendCancellation {
    private final Cancellation__c can;
    private final Cost_Note__c costnote;
    private final Claim__c claim;
    private final Opportunity opp;

    public SendCancellation(ApexPages.StandardController stdCtl) {
        this.can = [
            SELECT ID, Cost_Note__c
            FROM Cancellation__c
            WHERE Id = :stdCtl.getId()
        ];
        this.costnote = [
            SELECT Id, Claim__c
            FROM Cost_Note__c
            WHERE Id = :this.can.Cost_Note__c
        ];
        this.claim = [
            SELECT ID, Opportunity__c, Opportunity_Record_Type_Id__c, Opportunity_Legal_Account_Id__c
            FROM Claim__c
            WHERE ID = :this.costnote.Claim__c
        ];
        this.opp = [
            SELECT Id, Legal_Protection_Insurance__c
            FROM Opportunity
            WHERE Id = :this.claim.Opportunity__c
        ];
    }

    public PageReference renderPage() {
        /*
        if (getTpl() == '') {
            return Errors.page('Kein template gefunden.');
        }
        */
        if (this.opp.Legal_Protection_Insurance__c == null) {
            return Errors.page('Bitte hinterlegen Sie eine "Rechtsschutzversicherung" auf der Anfrage.');
        }
        string url = page.sendMail.getUrl() + createUrl();
        return new PageReference(url);
    }

    private string getTpl() {
        if (Opportunities.isDieselSkandal(this.claim.Opportunity_Record_Type_Id__c)) { // 0120Y000000W0CY
            if (Accounts.isJCLegal(this.claim.Opportunity_Legal_Account_Id__c)) { // 0010Y000011MYHW
                return EmailTemplates.idOf(EmailTemplates.Cancellation.DieselJaegerLegalCancellation);
            }
            if (Accounts.isLegalLeCom(this.claim.Opportunity_Legal_Account_Id__c)) {
                return EmailTemplates.idOf(EmailTemplates.Cancellation.DieselLeComLegalCancellation);
            }
            if (Accounts.isCLLBLegal(this.claim.Opportunity_Legal_Account_Id__c)) { // 0010Y000010COcm
                return EmailTemplates.idOf(EmailTemplates.Cancellation.DieselKainzLegalCancellation);
            }
            return '';
        }
        if (Opportunities.isFinanzierung(this.claim.Opportunity_Record_Type_Id__c)) { // 0120Y000000KnMH
            if (Accounts.isJCLegal(this.claim.Opportunity_Legal_Account_Id__c)) { // 0010Y000011MYHW
                return EmailTemplates.idOf(EmailTemplates.Cancellation.FzJaegerLegalCancellation);
            }
            if (Accounts.isLegalLeCom(this.claim.Opportunity_Legal_Account_Id__c)) {
                return EmailTemplates.idOf(EmailTemplates.Cancellation.FzLeComLegalCancellation);
            }
            if (Accounts.isCLLBLegal(this.claim.Opportunity_Legal_Account_Id__c)) { // 0010Y000010COcm
                return EmailTemplates.idOf(EmailTemplates.Cancellation.FzKainzLegalCancellation);
            }
            return '';
        }
        if (Accounts.isCLLBLegal(this.claim.Opportunity_Legal_Account_Id__c)) { // 0010Y000010COcm
            return EmailTemplates.idOf(EmailTemplates.Cancellation.LvKainzLegalCancellation);
        }
        return EmailTemplates.idOf(EmailTemplates.Cancellation.LvJaegerLegalCancellation);
    }

    private string createUrl() {
        return
            '?id=' + this.can.Id +
            '&tpl=' + getTpl() +
            '&recipient=' + this.opp.Legal_Protection_Insurance__c +
            '&noatt=false' + // when set to true it will not present the attachments
            '&hdl=Kunde informieren.';
    }
    
