    <validationRules>
        <fullName>Change_Stage_Sachbearbeiter_required</fullName>
        <active>true</active>
        <description>Bevor die Stage "Entwurf" verändert werden kann, muss ein Sachbearbeiter eingetragen werden.</description>
        <errorConditionFormula>AND($User.Bypass_validation_rule__c = false,
OR(ISPICKVAL ( Stage__c , "Customers Opposition"),
ISPICKVAL ( Stage__c , "Legal Opposition"),
ISPICKVAL ( Stage__c , "Claim Letter"),
ISPICKVAL ( Stage__c , "Legal Opposition"),
ISPICKVAL ( Stage__c , "Lawsuit"),
ISPICKVAL ( Stage__c , "Class Action"),
ISPICKVAL ( Stage__c , "Appeal"),
ISPICKVAL ( Stage__c , "Processing"),
ISPICKVAL ( Stage__c , "Finished"),
ISPICKVAL ( Stage__c , "On Hold"),
ISPICKVAL ( Stage__c , "Canceled")),	
ISBLANK( Clerk__c ))</errorConditionFormula>
        <errorDisplayField>Clerk__c</errorDisplayField>
        <errorMessage>Bitte tragen Sie einen Sachbearbeiter ein.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CheckOpposingCounselAccountId</fullName>
        <active>true</active>
        <errorConditionFormula>AND($User.Bypass_validation_rule__c = false,
Opportunity__r.Vehicle_Brand__r.Id !=  Opposing_Counsel__r.AccountId,Opposing_Counsel__r.AccountId!=null)</errorConditionFormula>
        <errorDisplayField>Opposing_Counsel__c</errorDisplayField>
        <errorMessage>Korrespondenzanwalt muss Fahrzeughersteller zugeordnet sein.</errorMessage>
    </validationRules>
